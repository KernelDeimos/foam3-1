p({
  "class":"foam.nanos.boot.NSpec",
  "name":"localSpidGroupURLDAO",
  "description":"URL by spid and group",
  "authenticate": false,
  "serviceScript":
  """
    return new foam.dao.EasyDAO.Builder(x)
      .setOf(foam.nanos.auth.SpidGroupURL.getOwnClassInfo())
      .setJournalName("spidGroupURLs")
      .setJournalType(foam.dao.JournalType.SINGLE_JOURNAL)
      .setPm(true)
      .build();
  """
})

p({
  "class":"foam.nanos.boot.NSpec",
  "name":"spidGroupURLDAO",
  "description":"URL by spid and group",
  "serve": true,
  "authenticate": true,
  "serviceScript":
  """
    return new foam.dao.EasyDAO.Builder(x)
      .setOf(foam.nanos.auth.SpidGroupURL.getOwnClassInfo())
      .setInnerDAO(x.get("localSpidGroupURLDAO"))
      .setPm(true)
      .setEnableInterfaceDecorators(false)
      .build();
  """,
  "client": """
  {
    "of": "foam.nanos.auth.SpidGroupURL"
  }
  """
})
