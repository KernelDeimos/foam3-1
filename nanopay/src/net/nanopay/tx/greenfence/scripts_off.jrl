p({"class":"foam.nanos.script.Script","id":"Greenfence - Invoice Creation","code":"import net.nanopay.tx.GreenfenceTransaction;\nimport net.nanopay.tx.TransactionLineItem;\import net.nanopay.tx.ExpenseLineItem;\nimport net.nanopay.tx.ServiceLineItem;\n\ngreenTxn = new GreenfenceTransaction();\ngreenTxn.setPayerId(1015L);\ngreenTxn.setPayeeId(1014L);\nlineItems = new TransactionLineItem[] {new ServiceLineItem.Builder(x).setId(java.util.UUID.randomUUID().toString()).setType(\"f269f779-9ecf-473f-99cc-25b3d19f9851\").setAmount(500000).build(), new ExpenseLineItem.Builder(x).setId(java.util.UUID.randomUUID().toString()).setType(\"1a77434b-5e83-4fc3-989b-3527b2f40a33\").setAmount(100000).build()};\ngreenTxn.setLineItems(lineItems);\ngreenTxn =  x.get(\"localTransactionDAO\").put(greenTxn);\nprint(greenTxn);\n"})

