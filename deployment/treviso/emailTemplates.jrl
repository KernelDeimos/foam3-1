p({
  "class": "foam.nanos.notification.email.EmailTemplate",
  "id": "4f45c745-1c93-4380-b275-2d2fea091d1f-118",
  "name": "invoice-approval-email",
  "group": "sme",
  "displayName": "{{appName}}",
  "subject": "{{fromName}} sent an invoice for you to approve",
  "body": "\n\n<include template = 'treviso-email-base_2'>\n <p style=\"font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; font-size: 24px; font-weight: 900; margin: 40px 0;\">Aprovação do pagamento ou remessa enviado para {{paymentTo}}</p>\n <p style = \"font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin: 0;\"> Olá {{name}},</p>\n <br>\n <p style = \"font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin-top: 0; margin-bottom: 40px;\">{{fromName}} Está enviando um pagamento para {{paymentTo}}, e necessita sua aprovação. Por favor logue no portal da {{appName}} para aprovar o pagamento. </p>\n {% if OperationCode != '' %}\n <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px;\">\n <div class= \"left  \" style= \"float:left;  font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">\n <b>Código da Operação</b>\n </div>\n <div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">\n {{OperationCode}}\n </div>\n </div>\n {% endif %}\n <div style= \"border-radius: 4px; font-size: 14px;\"> <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px;\"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \"><b>Pagamento para</b></div><div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">{{paymentTo}}</div></div>\n <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px; padding-top: 8px;\"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  \"><b>Número da Fatura</b></div><div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">#{{account}}</div></div>\n <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px; padding-top:8px  \"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;   \"><b>Valor Pago</b></div> <div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">{{amount}} {{currency}}</div></div>\n <div style= \"overflow: hidden; padding-bottom: 8px; padding-top:8px  \"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  \"> <b>Data de Vencimento</b></div><div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">{{date}} </div> </div></div>\n <a href=\"{{link}}\" style=\"text-decoration: none;\"><div style=\"width: 100%; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; margin: 40px 0; height: 48px; border-radius: 3px; background: #6c54ff; font-size: 16px; color: #fff; cursor: pointer; text-align: center;\">\n <p style = \"padding: 14px 0 0; margin: 0;\">Visualizar fatura</p>\n </div></a>\n \n <p style=\"font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; font-size: 24px; font-weight: 900; margin: 40px 0;\">Approve payment for {{paymentTo}}</p>\n <p style = \"font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin: 0;\"> Hello {{name}},</p>\n <br>\n <p style = \"font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin-top: 0; margin-bottom: 40px;\">{{fromName}} has sent a payment for {{paymentTo}} for your approval. Please approve the payment in {{appName}} for this invoice to be paid. </p>\n {% if OperationCode != '' %}\n <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px;\">\n <div class= \"left  \" style= \"float:left;  font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">\n <b>Operation Code</b>\n </div>\n <div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">\n {{OperationCode}}\n </div>\n </div>\n {% endif %}\n <div style= \"border-radius: 4px; font-size: 14px;\"> <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px;\"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \"><b>Payment to</b></div><div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">{{paymentTo}}</div></div>\n <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px; padding-top: 8px;\"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  \"><b>Invoice No.</b></div><div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">#{{account}}</div></div>\n <div style= \"overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px; padding-top:8px  \"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;   \"><b>Amount paid</b></div> <div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">{{amount}} {{currency}}</div></div>\n <div style= \"overflow: hidden; padding-bottom: 8px; padding-top:8px  \"><div class= \"left  \" style= \"float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  \"> <b>Date Due</b></div><div class= \"right  \" style= \"float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; \">{{date}} </div> </div></div>\n <a href=\"{{link}}\" style=\"text-decoration: none;\"><div style=\"width: 100%; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; margin: 40px 0; height: 48px; border-radius: 3px; background: #6c54ff; font-size: 16px; color: #fff; cursor: pointer; text-align: center;\">\n <p style = \"padding: 14px 0 0; margin: 0;\">View invoice</p>\n </div></a>\n \n </include>"
})
p({
  "class": "foam.nanos.notification.email.EmailTemplate",
  "id": "4f45c745-1c93-4380-b275-2d2fea091d1f-119",
  "name": "invoice-transaction-completed",
  "group": "sme",
  "locale": "en",
  "displayName": "{{appName}}",
  "subject": "The payment from {{fromName}} has been deposited using {{appName}}",
  "body": """
    <include template = 'treviso-email-base_2'>
      <p style ="margin-top: -46px; float:right; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; font-size:12px; color:#8e9090;">Transaction ID: {{transId}}</p>
      <p style="font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; font-size: 24px; font-weight: 900; margin: 40px 0;">Successfully deposited payment from {{fromName}}</p>
      <p style = "font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin: 0;"> Hello {{name}},</p>
      <br>
      <p style = "font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin: 0; margin-bottom: 40px;">The payment for {{amount}} {{currency}} sent from {{fromName}} has been successfully deposited to your bank account.</p>
      <div style= "border-radius: 4px; font-size: 14px;">
        {% if OperationCode != '' %}
        <div style= "overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px;">
          <div class= "left  " style= "float:left;  font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; ">
            <b>Operation Code</b>
          </div>
          <div class= "right  " style= "float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; ">
            {{OperationCode}}
          </div>
        </div>
        {% endif %}
        <div style= "overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px;">
          <div class= "left  " style= "float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;"><b>Payment from</b></div>
          <div class= "right  " style= "float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;">{{fromName}}</div>
        </div>
        {% if account != '' %}
        <div style= "overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px; padding-top: 8px;">
          <div class= "left  " style= "float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; "><b>Invoice No.</b></div>
          <div class= "right  " style= "float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;">#{{account}}</div>
        </div>
        {% endif %}
        <div style= "overflow: hidden; border-bottom: 1px solid #d9d9d9; padding-bottom: 8px; padding-top:8px  ">
          <div class= "left  " style= "float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; "><b>Amount paid</b></div>
          <div class= "right  " style= "float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;">{{amount}}{{currency}}</div>
        </div>
        <div style= "overflow: hidden; padding-bottom: 8px; padding-top:8px  ">
          <div class= "left  " style= "float:left; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; "> <b>Date paid</b></div>
          <div class= "right  " style= "float:right; color: #8e9090; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;">{{date}} </div>
        </div>
      </div>
      {% if currency != 'INR' %}
      <a href="{{link}}" style="text-decoration: none;">
        <div style="width: 100%; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; margin: 20px 0 0 0; height: 48px; border-radius: 3px; background: #6c54ff; font-size: 16px; color: #fff; cursor: pointer; text-align: center;">
          <p style = "padding: 14px 0 0; margin: 0;">View invoice</p>
        </div>
      </a>
      {% endif %}
      <p style = "font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif;  font-size: 16px;  line-height: 1.5; margin: 20px 0 0 0;">If you have any questions about this payment, please contact {{fromName}} directly.</p>
    </include>
  """
})
p({
  "class": "foam.nanos.notification.email.EmailTemplate",
  "id": "4f45c745-1c93-4380-b275-2d2fea091d1f-119-pt",
  "name": "invoice-transaction-completed",
  "group": "sme",
  "locale": "pt",
  "displayName": "{{appName}}",
  "subject": "O pagamento de {{fromName}} foi depositado usando {{appName}}",
  "body": """
    <include template = 'treviso-email-base_2'>
