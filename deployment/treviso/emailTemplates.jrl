  "sendTo": "{{userEmail}}",
  "group": "sme",
  "locale": "en",
  "displayName": "{{appName}}",
  "subject": "Bank account verified",
  "body": """
    <include template = 'treviso-email-base_2'>
      <p style="font-size: 24px; font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; margin-top: 50px;margin-bottom: 40px;"><b>Bank account verified</b></p>
      <p style = "font-size: 16px;"> Hi {{name}},</p>
      <br>
      <p style = "font-family: /*%FONT1%*/ Roboto, 'Helvetica Neue', Helvetica, Arial, sans-serif; font-size: 16px; margin-top: -5px; margin-bottom: 30px;">Your bank account has been successfully verified:</p>
      <br>
      {% if accountType == 'CABankAccount' %}
      <div style= "border-radius: 4px; font-size: 14px;">
